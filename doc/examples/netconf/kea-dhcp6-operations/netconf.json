{
    "Netconf":
    {
        "managed-servers":
        {
            "dhcp6":
            {
                "control-socket":
                {
                    "socket-type": "unix",
                    "socket-name": "/tmp/kea6-sock"
                }
            }
        }
    },
    "Logging":
    {
        "loggers":
        [
            {
                "name": "kea-netconf",
                "output_options":
                [
                    {
                        "output": "stderr"
                    }
                ],
                "severity": "DEBUG",
                "debuglevel": 99
            }
        ]
    }
}
