{
    "name": "network4-del",
    "brief": "The network4-del command is used to delete existing shared networks.",
    "description": "See <xref linkend=\"idp76\"/>",
    "support": [ "kea-dhcp4", "kea-dhcp6" ],
    "avail": "1.3.0",
    "hook": "subnet_cmds",
    "cmd-syntax": "{
    \"command\": \"network4-del\",
    \"arguments\": {
        \"name\": \"floor13\"
    }
}",
    "resp-syntax": "{
    \"arguments\": {
        \"shared-networks\": [
            {
                \"name\": \"floor13\"
            }
        ]
    },
    \"result\": 0,
    \"text\": \"IPv4 shared network 'floor13' deleted\"
}"
}
