{
    "version": 3,
    "Logging": {
        "loggers": [ {
            "debuglevel": 99,
            "severity": "DEBUG",
            "name": "*"
        } ]
    },
    "Auth": {
        "database_file": "data/root.sqlite3",
        "listen_on": [ {
            "port": 47806,
            "address": "127.0.0.1"
        } ]
    },
    "data_sources": {
        "classes": {
            "IN": [
                {
                    "type": "sqlite3",
                    "cache-enable": false,
                    "params": {
                        "database_file": "data/root.sqlite3"
                    }
                }
            ]
        }
    },
    "Stats": {
        "poll-interval": 60
    },
    "Init": {
        "components": {
            "b10-auth": { "kind": "needed", "special": "auth" },
            "b10-stats": { "address": "Stats", "kind": "dispensable" },
            "b10-cmdctl": { "special": "cmdctl", "kind": "needed" }
        }
    }
}
