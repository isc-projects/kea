    "description": "See <xref linkend=\"idp68\"/>",
    "support": [ "kea-dhcp6" ],
    "avail": "1.3.0",
    "hook": "subnet_cmds",
    "cmd-syntax": "{
    \"command\": \"subnet6-get\",
    \"arguments\": {
        \"id\": 11
    }
}",
    "resp-syntax": "{
    \"result\": 0,
    \"text\": \"Info about IPv6 subnet 2001:db8:1::/64 (id 11) returned\",
    \"arguments\": {
        \"subnets\": [
            {
                \"subnet\": \"2001:db8:1::/64\",
                \"id\": 1,
                \"option-data\": [
                    ...
                ]
                ....
            }
        ]
    }
}"
}
