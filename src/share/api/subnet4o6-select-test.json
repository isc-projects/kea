{
    "access": "read",
    "avail": "2.7.0",
    "brief": [ "This command returns the result of DHCPv4o6 subnet selection" ],
    "cmd-comment": [ "Possible parameters are the remote address, interface id option, interface name (the three from the DHCPv6 server), etc." ],
    "cmd-syntax": [
        "{",
        "    \"command\": \"subnet4o6-select-test\",",
        "    \"arguments\": {",
        "        \"interface\": <string>,",
        "        \"interface-id\": <string>,",
        "        \"address\": <string>,",
        "        \"relay\": <string>,",
        "        \"local\": <string>,",
        "        \"remote\": <string>,",
        "        \"link\": <string>,",
        "        \"subnet\": <string>,",
        "        \"classes\": [ <strings> ]",
        "    }",
        "}"
    ],
    "description": [ "See <xref linkend=\"command-subnet4o6-select-test\"/>" ],
    "name": "subnet4o6-select-test",
    "resp-comment": [ "3 forms for selected shared network, subnet or nothing.." ],
    "resp-syntax": [ "{ \"result\": 1, \"text\": <localization result> }" ],
    "support": [ "kea-dhcp4" ]
}
