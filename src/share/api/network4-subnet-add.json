    ],
    "cmd-syntax": [
        "{",
        "    \"command\": \"network4-subnet-add\",",
        "    \"arguments\": {",
        "        \"name\": \"floor13\",",
        "        \"id\": 5",
        "    }",
        "}"
    ],
    "description": "See <xref linkend=\"idp77\"/>",
    "hook": "subnet_cmds",
    "name": "network4-subnet-add",
